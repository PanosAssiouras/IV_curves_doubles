<?xml version="1.0" encoding="UTF-8"?>
<Plot
	PlotFlag	 = "true"
	SaveFlag    	 = "true"
	SaveFormat	 = "pdf,png"
	SaveFolder  	 = "double_crocv2_20/"
	FileName    	 = "3D_ITSensorIV"
	FileHeader	 = "Voltage,Current"
	MatplotlibFlag	 = "true"
	>
</Plot>
<Devices>
	<PowerSupply 
		ID	    = "MyKeithley"
		InUse       = "Yes"
		Model       = "Keithley" 
		Connection  = "Serial"
		Port        = "/dev/ttyUSB0"
		BaudRate    = "9600"
		FlowControl = "false"
		Parity	    = "false"
		RemoveEcho  = "false"
		Terminator  = "LF"
		Suffix	    = "LF"
		Timeout	    = "1"
		>
	<Channel ID="Front" Channel="FRON" InUse ="Yes"/>
	<Channel ID="Rear" Channel="REAR" InUse ="No"/>
</PowerSupply>
</Devices>
<Options	
	Polarity	        = "negative"
	Compliance 	        = "5.e-6"
	CurrentReadRangeAmp     = "-1"
	waitTimeSeconds         = "2"
	VoltStepSize	        = "5"
	Vmax		        = "100"
	Panel		        = "front"
	VoltStepSizeRampDown    = "10"
	nPointsPerVolt	        = "5"
	>		
</Options>
